<h1><%= @member.last_name_kana %> <%= @member.first_name_kana %>さんの登録情報編集</h1>
<%= form_with model:@member,url: admin_member_path, method: :patch, local:true do |f| %>
<table>
  <tr>
    <th>
      <p>会員ID <%=@member.id %></p>
    </th>
  </tr>
  <tr>
    <th>
      <%= f.label :氏名 %>
      <%= f.text_field :last_name %>
      <%= f.text_field :first_name %>
    </th>
  </tr>
  <tr>
    <th>
      <%= f.label :フリガナ %>
      <%= f.text_field :last_name_kana %>
      <%= f.text_field :first_name_kana %>
    </th>
  </tr>
  <tr>
    <th>
      <%= f.label :郵便番号 %>
      <%= f.text_field :postal_code %>
    </th>
  </tr>
  <tr>
    <th>
      <%= f.label :住所 %>
      <%= f.text_field :address %>
    </th>
  </tr>
  <tr>
    <th>
      <%= f.label :電話番号 %>
      <%= f.text_field :telephone_number %>
    </th>
  </tr>
  <tr>
    <th>
      <%= f.label :メールアドレス %>
      <%= f.text_field :email %>
    </th>
  </tr>
  <tr>
    <th>
      <%= f.label :会員ステータス %>
      <%= f.radio_button "is_deleted", false %>有効<%= f.radio_button "is_deleted", true %>退会
    </th>
  </tr>
  
</table>
  <%= f.submit "変更内容を保存" %>
<% end %>
